{
  "manifest_version": 3,
  "name": "HJ EVA",
  "version": "1.0",
  "version_name": "1.0-beta",
  "description": "A utility for HelloJio EVA content team to provide json validation, hj intent schema validation and much more.",
  "author": "shrinath.gupta@ril.com",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/standaloneIntentValidation.js"],
      "matches": [
        "https://eva-replica.hellojio.jio.com/portal/home/bots/*/intent/*"
      ]
    }
  ]

}